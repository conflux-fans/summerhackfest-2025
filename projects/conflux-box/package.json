{
  "name": "conflux-box",
  "version": "0.1.0",
  "description": "Modern Conflux Development Kit Frontend - Built with Mantine UI and the published Conflux DevKit packages",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"vite\"",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "backend": "node backend-service.js",
    "start:backend": "node backend-service.js",
    "start:dev": "concurrently \"npm run backend\" \"npm run dev:frontend\"",
    "start": "npm run start:dev"
  },
  "dependencies": {
    "@conflux-devkit/backend": "^0.1.0",
    "@conflux-devkit/node": "^0.1.0",
    "@mantine/core": "^7.12.0",
    "@mantine/dates": "^7.12.0",
    "@mantine/form": "^7.12.0",
    "@mantine/hooks": "^7.12.0",
    "@mantine/modals": "^7.12.0",
    "@mantine/notifications": "^7.12.0",
    "@mesonfi/to": "^2.0.0-rc.2",
    "@tabler/icons-react": "^3.10.0",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "connectkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "viem": "^2.37.9",
    "wagmi": "^2.17.5",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.32",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "solc": "^0.8.30",
    "typescript": "^5.2.2",
    "vite": "^5.0.5",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "conflux",
    "blockchain",
    "devkit",
    "mantine",
    "react",
    "typescript",
    "frontend"
  ],
  "author": "Conflux DevKit Team",
  "license": "Apache-2.0"
}
